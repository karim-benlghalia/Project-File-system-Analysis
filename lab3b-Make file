UID=105179657
default:build

build:
	ln file lab3b
	chmod u+x lab3b
	find . -type f | xargs touch


dist:
	tar -cvzf lab3b-$(UID).tar.gz Makefile README lab3b.py file
	find . -type f | xargs touch


clean:
	rm -f lab3b lab3b-$(UID).tar.gz
	find . -type f | xargs touch
